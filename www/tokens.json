{
  "$name": "Cobalt",
  "color": {
    "$type": "color",
    "black":     { "$value": "#00193f" },
    "blue":      { "$value": "#2b3faa" },
    "darkGray":  { "$value": "#282a37" },
    "green":     { "$value": "#77cd8f" },
    "purple":    { "$value": "#6a35d9" },
    "red":       { "$value": "#f6566a" },
    "white":     { "$value": "#ffffff" }
  },
  "font": {
    "family": {
      "$type": "font",
      "ppNeue": { "$value": ["PP Neue Montreal", "system-ui", "ui-sans-serif", "-apple-system", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"] },
      "mono":   { "$value": ["IBM Plex Mono", "ui-monospace", "SF Mono", "Monaco", "Cascadia Mono", "Cascadia Code", "Consolas", "monospace"] }
    },
    "size": {
      "$type": "dimension",
      "01": { "$value": "8px" },
      "02": { "$value": "9px" },
      "03": { "$value": "12px" },
      "04": { "$value": "14px" },
      "05": { "$value": "18px" },
      "06": { "$value": "24px" },
      "07": { "$value": "36px" },
      "08": { "$value": "56px" }
    }
  },
  "space": {
    "$description": "IBM Carbon spacing",
    "$type": "dimension",
    "4xs": { "$value": "1px" },
    "3xs": { "$value": "2px" },
    "2xs": { "$value": "4px" },
    "xs": { "$value": "8px" },
    "sm": { "$value": "12px" },
    "md": { "$value": "16px" },
    "lg": { "$value": "24px" },
    "xl": { "$value": "32px" },
    "2xl": { "$value": "40px" },
    "3xl": { "$value": "48px" },
    "layout": {
      "2xs": { "$value": "16px" },
      "xs": { "$value": "24px" },
      "sm": { "$value": "32px" },
      "md": { "$value": "48px" },
      "lg": { "$value": "64px" },
      "xl": { "$value": "96px" },
      "2xl": { "$value": "160px" }
    }
  }
}
